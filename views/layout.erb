      <li<% if request.path_info.include? 'presentations' %> class="current"<% end %>><a href="/presentations">Presentations</a></li>
      <li<% if request.path_info.include? 'recipes' %> class="current"<% end %>><a href="/recipes">Recipes</a></li>
      <li><a href="<%= Serious.feed_url %>">RSS</a></li>
      <li<% if request.path_info.include? 'about' %> class="current"<% end %>><a href="/about">About</a></li>
      <li><a href="mailto:jjfutbol@gmail.com">Contact</a></li>
    </ul>
    
  </nav>
  
  <section id="content">
  
    <div class="inner">
      
      <% if @contentTitle %><h2><%= @contentTitle %></h2><% end %>
      
      <%= yield %>
      
